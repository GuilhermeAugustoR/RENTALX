    "paths": {
        "/categories": {
            "post": {
                "tags": [
                    "Category"
                ],
                "summary": "Create a category",
                "description": "Create a new Category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "category name sample",
                                    "description": "Category description sample"
                                }
                            }
                        }
                    }
                },
                "response": {
                    "201": {
                        "description": "Created"
                    },
                    "500": {
                        "description": "Category alredy exist"
                    }
                }
            },
            "get": {
                "tags": [
                    "Category"
                ],
                "summary": "List all categories",
                "description": "List all categories",
                "response": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/categories/import": {
            "post": {
                "tags": [
                    "Category"
                ],
                "summary": "Import categories",
                "description": "Import categories",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    }
                }
            }
        }
    },
    "/specifications": {
        "post": {
            "tags": [
                "Specification"
            ],
            "summary": "Create a specification",
            "description": "Create a new Specification",
            "requestBody": {
                "content": {
                    "application/json": {
                        "schema": {
                            "$ref": "#/definitions/Specification"
                        }
                    }
                }
            }
        },
        "response": {
            "201": {
                "description": "Created"
            },
            "500": {
                "description": "Specification alredy exist"
            }
        }
    },
    "definitions": {
        "Specification": {
            "type": "object",
            "properties": {
